###########################################################################
# General parameters
###########################################################################

name = d20

###########################################################################
# Sieve
###########################################################################

tasks.I = 13
tasks.lim0 = 800
tasks.lpb0 = 10
tasks.sieve.mfb0 = 20
tasks.sieve.qrange = 25
